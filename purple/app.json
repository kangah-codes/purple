{
  "expo": {
    "name": "Purple",
    "slug": "spendwise",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "width": 320,
      "height": 320
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.akangah89.Purple"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#9333EA"
      },
      "package": "com.akangah89.Purple"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Suprapower.otf",
            "./assets/fonts/Inter-Thin.ttf",
            "./assets/fonts/Inter-Black.ttf",
            "./assets/fonts/Inter-Bold.ttf",
            "./assets/fonts/Inter-ExtraBold.ttf",
            "./assets/fonts/Inter-ExtraLight.ttf",
            "./assets/fonts/Inter-Light.ttf",
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/Wolf-Sans-Regular.ttf",
            "./assets/fonts/Arial-Rounded-MT-Bold.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e315a942-184e-49ce-a243-e9006ec7ddb1"
      }
    }
  }
}
